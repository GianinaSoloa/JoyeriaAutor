  return (cartItems.some (itemInCart => itemInCart.id === Number(id)));\n    }\n\n\n    const clear = () => {\n        setCartItems([]);\n    }\n\n    return(\n        <CartContext.Provider value={{cartItems, qtyProducts, addToCart, removeItem, isInCart, clear}}>\n            {children}\n        </CartContext.Provider>\n    )\n}\n\nexport default CartProvider;","/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Data/data.js",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Contact/UserForm.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/CartWidget/CartWidget.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/ItemList/ItemList.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/ItemDetail/ItemDetail.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Item/Item.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/ItemCount/ItemCount.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/services/firebase.js",["192"],[],{"ruleId":"193","severity":1,"message":"194","line":15,"column":66,"nodeType":"195","messageId":"196","endLine":15,"endColumn":68},{"ruleId":"197","severity":1,"message":"198","line":29,"column":5,"nodeType":"199","endLine":29,"endColumn":7,"suggestions":"200"},{"ruleId":"197","severity":1,"message":"201","line":17,"column":8,"nodeType":"199","endLine":17,"endColumn":19,"suggestions":"202"},{"ruleId":"203","severity":1,"message":"204","line":6,"column":8,"nodeType":"205","messageId":"206","endLine":6,"endColumn":19},{"ruleId":"203","severity":1,"message":"207","line":7,"column":10,"nodeType":"205","messageId":"206","endLine":7,"endColumn":17},{"ruleId":"203","severity":1,"message":"208","line":7,"column":19,"nodeType":"205","messageId":"206","endLine":7,"endColumn":29},{"ruleId":"203","severi
ollection, doc } from 'firebase/firestore';\n\n\nconst ItemDetailContainer = () =>{\nconst [item, setItem] = useState({});\nconst {id} = useParams();\n\nuseEffect(() => {\n            const getItemsById = (id) => {\n                return new Promise((resolve, reject) => {\n                    const products = collection(firestoreDB, \"joyas\");\n                    const docRef = doc(products, id);\n\n                    getDoc(docRef).then( snapshot => {\n                        resolve(\n                            {...snapshot.data(), id: snapshot.id}\n                        )\n                    });\n                })\n            }    \n        \n\n            getItemsById(id).then(\n                (response) => {\n                    setItem(response);\n                }\n                ).catch(\n                (error) => {\n                    console.error(error);\n                }\n            )\n        },\n    []\n)\n\n    return(\n        <div className='container__item'>\n            <h1>Detalle</h1>\n            <ItemDetail\n                key = {item?.id}\n                id = {item?.id}\n                stock = {item?.stock}\n                title = {item?.title}\n                description = {item?.description}\n                price = {item?.price}\n                pictureUrl = {item?.pictureUrl}\n            />\n        </div>\n    )\n}\n\nexport default ItemDetailContainer;","/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Cart/Cart.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Contact/UserForm.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/CartWidget/CartWidget.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/ItemList/ItemList.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/ItemDetail/ItemDetail.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/r
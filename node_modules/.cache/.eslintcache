uantity)\n        setQtyProducts(quantity)\n    }\n\n    useEffect(() => {\n        getQtyProducts();\n    }, [cartItems]\n\n    )\n\n    const addToCart = (item) => {\n        let cartCopy = [...cartItems];\n        if (isInCart(item.id)) {\n            const found = cartItems.find (i => i.id === Number(item.id));\n            const index = cartItems.indexOf(found);\n            cartCopy[index].quantity += item.quantity;\n            setCartItems(cartCopy)\n        } else {\n            cartCopy.push({...item});\n            setCartItems(cartCopy)\n        };\n    }\n\n    const removeItem = (id) =>{\n        setCartItems (cartItems.filter((item) => item.id !== id));\n    }\n\n    const isInCart = (id) => {\n        return (cartItems.some ((itemInCart) => itemInCart.id === Number(id)));\n    }\n\n\n    const clear = () => {\n        setCartItems([]);\n    }\n\n    const subtotalPrice = (price, quantity) => {\n        let subtotal = (price * quantity);\n        return subtotal;\n    }\n\n    const totalPrice = () => {\n        let total = 0;\n        cartItems.forEach ( (i) => total += i.price * i.quantity );\n        return total;\n    \n    }\n    \n\n    const totalItems = () => {\n        let totalQty = 0;\n        cartItems.map(i => totalQty += i.quantity);\n        return totalQty;\n    }\n\n    return(\n        <CartContext.Provider value={{cartItems, qtyProducts, addToCart, removeItem, isInCart, clear, totalPrice, subtotalPrice, totalItems}}>\n            {children}\n        </CartContext.Provider>\n    )\n}\n\nexport default CartProvider;","/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Us/Slider.jsx",[],[],"/Users/Giani/Documents/CURSO-CODER/React/react/joyeria-autor/src/Components/Spinner/Spinner.jsx",[],[],{"ruleId":"216","severity":1,"message":"217","line":15,"column":66,"nodeType":"218","messageId":"219","endLine":15,"endColumn":68},{"ruleId":"220","severity":1,"message":"221","line":29,"column":5,"nodeType":"222","endLine"